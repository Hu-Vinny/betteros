{
  "manifest_version": 3,
  "name": "BetterOS",
  "description": "QoL improvements for Online Sequencer!",
  "version": "1.0",

  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://onlinesequencer.net/*"],
      "js": [
       "content/menuadd.js"
//        "content/lightmode.js"
      ]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["page.js"],
      "matches": ["https://onlinesequencer.net/*"]
    }
  ]
}
